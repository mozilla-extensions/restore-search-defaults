{
  "manifest_version": 2,
  "name": "Reset Search Defaults",
  "applications": {
    "gecko": {
      "id": "reset-search-defaults@mozilla.com",
      "strict_min_version": "81.0",
      "strict_max_version": "86.0"
    }
  },
  "version": "1.0.5",
  "description": "Ask the user if they would like to use a specified search engine as the default.",
  "experiment_apis": {
    "search": {
      "schema": "schema.json",
      "parent": {
        "scopes": ["addon_parent"],
        "script": "api.js",
        "events": ["startup"]
      }
    }
  }
}
